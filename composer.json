{
    "name": "vdhicts/rebrandly-api-client",
    "description": "API client for the API of Rebrandly",
    "license": "MIT",
    "authors": [
        {
            "name": "Dick van der Heiden",
            "email": "info@vdhicts.nl"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "nesbot/carbon": "^2.31"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Vdhicts\\Rebrandly\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vdhicts\\Rebrandly\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
